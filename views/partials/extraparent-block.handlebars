        <div id="extra_parent_div" class="form-row mx-5 mt-3">
          <div class="form-group col-md-6">
            <label class="form-label">Parents</label>
            <select required id="parents_select" data-parents_select="1" name="parents_select" class="selectpicker" data-live-search="true" data-style="btn-outline-secondary" data-width="100%" title="Choose one parent from this list." >
              {{#each parents}}
                <option value="{{this.id}}">{{this.last_name}}, {{this.first_name}}</option>
              {{/each}}
            </select>
          </div>
          <!--Relationship-->
          <div class="form-group col-md-4">
            <label class="form-label" for="relationship">Relationship</label>
            <input type="text" class="form-control" data-relationship="1" name="relationship" id="relationship" maxlength="35" placeholder="Father, Mother, Step, etc." required>
            <p class="text-right feedback" id="feedback_relationship">35 characters remain</p>
          </div>
          <!--Sequence-->
          <div class="form-group col-md-2">
            <label class="form-label" for="contact_sequence">Contact Order</label>
            <input type="number" class="form-control" data-contact_sequence="1" name="contact_sequence" id="contact_sequence" min="1" step="1" required>
          </div>
        </div>